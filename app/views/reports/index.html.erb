<h1>一覧</h1>

<%= link_to '新規', new_report_path %>
<ul>
  <% @reports.each do |report| %>
    <li>
      <ul>
        <li><%= report.note %></li>
        <li><%= report.created_at %></li>
        <li><%= link_to '詳細', report_path(report) %><%= link_to '編集', edit_report_path(report) %></li>
      </ul>

    </li>
  <% end %>
</ul>

